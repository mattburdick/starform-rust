    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug 'starform -v 3'",
            "env": {
                "RUST_BACKTRACE": "1"
            },
            "cargo": {
                "args": [
                    "build",
                    "--bin=starform-rust",
                    "--package=starform-rust"
                ],
                "filter": {
                    "name": "starform-rust",
                    "kind": "bin"
                }
            },
            "args": [
                "-v",
                "3"
            ],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug 'starform -v 3 -t G3M/1'",
            "env": {
                "RUST_BACKTRACE": "1"
            },
            "cargo": {
                "args": [
                    "build",
                    "--bin=starform-rust",
                    "--package=starform-rust"
                ],
                "filter": {
                    "name": "starform-rust",
                    "kind": "bin"
                }
            },
            "args": [
                "-v",
                "3",
                "-t",
                "G3M/1"
            ],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug 'starform -v 3 -t G3M/1 -d 2'",
            "env": {
                "RUST_BACKTRACE": "1"
            },
            "cargo": {
                "args": [
                    "build",
                    "--bin=starform-rust",
